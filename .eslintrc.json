{
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/no-cycle": "off",
      "consistent-return": "off",
      "no-await-in-loop": "off",
      "no-continue": "off",
      "no-new": "off",
      "no-console": "off",
      "no-empty-function": "off",
      "no-empty": "off"
    }
}
